<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BookingExpandedActivity">

    <ImageView
        android:id="@+id/imageHotelExpanded"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="250dp"
        android:scaleType="fitXY"
        android:src="@drawable/hotelexample"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/backBookingsButton"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/backicn"
        android:scaleType="fitStart"
        app:layout_constraintBottom_toTopOf="@+id/nameHotelTextView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/locationHotelTextView2"
        style="@style/TextAppearance.AppCompat.Title"
        android:layout_width="158dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/sfprodisplaylight"
        android:text="@string/espa_a"
        android:textAlignment="center"
        android:textColor="#81C3F8"
        android:textSize="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nameHotelTextView2" />

    <TextView
        android:id="@+id/nameHotelTextView2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/sfprodisplayheavy"
        android:text="@string/holiday_inn_hotel"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_centerInParent="true"
        android:layout_marginTop="16dp"
        android:background="@drawable/keyimg"
        android:elevation="7dp"
        android:src="@drawable/key"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="@+id/mainDoorTextView"
        app:layout_constraintStart_toStartOf="@+id/mainDoorTextView"
        app:layout_constraintTop_toBottomOf="@+id/open1TextView" />

    <ImageView
        android:id="@+id/unlockRoomImage"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_centerInParent="true"
        android:layout_marginTop="16dp"
        android:background="@drawable/keyimg"
        android:elevation="7dp"
        android:src="@drawable/key"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="@+id/roomTextView2"
        app:layout_constraintStart_toStartOf="@+id/roomTextView2"
        app:layout_constraintTop_toBottomOf="@+id/open2TextView2" />

    <TextView
        android:id="@+id/roomTextView2"
        style="@style/TextAppearance.AppCompat.Title"
        android:layout_width="83dp"
        android:layout_height="0dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="60dp"
        android:fontFamily="@font/sfprodisplaymedium"
        android:text="@string/habitaci_n_124"
        android:textAlignment="center"
        android:textColor="#2D508E"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageHotelExpanded" />

    <TextView
        android:id="@+id/open2TextView2"
        style="@style/TextAppearance.AppCompat.Title"
        android:layout_width="83dp"
        android:layout_height="0dp"
        android:fontFamily="@font/sfprodisplaylight"
        android:text="@string/abrir"
        android:textAlignment="center"
        android:textColor="#81C3F8"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="@+id/roomTextView2"
        app:layout_constraintStart_toStartOf="@+id/roomTextView2"
        app:layout_constraintTop_toBottomOf="@+id/roomTextView2" />

    <TextView
        android:id="@+id/mainDoorTextView"
        android:layout_width="83dp"
        android:layout_height="0dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/sfprodisplaymedium"
        android:text="@string/puerta_principal"
        android:textAlignment="center"
        android:textColor="#2D508E"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageHotelExpanded" />

    <TextView
        android:id="@+id/open1TextView"
        style="@style/TextAppearance.AppCompat.Title"
        android:layout_width="83dp"
        android:layout_height="0dp"
        android:fontFamily="@font/sfprodisplaylight"
        android:text="@string/abrir"
        android:textAlignment="center"
        android:textColor="#81C3F8"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="@+id/mainDoorTextView"
        app:layout_constraintStart_toStartOf="@+id/mainDoorTextView"
        app:layout_constraintTop_toBottomOf="@+id/mainDoorTextView" />

    <View
        android:id="@+id/view"
        android:layout_width="0dp"
        android:layout_height="105dp"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="16dp"
        android:foreground="@color/design_default_color_primary_dark"
        app:layout_constraintBottom_toBottomOf="@+id/imageHotelExpanded"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/locationHotelTextView3"
        style="@style/TextAppearance.AppCompat.Title"
        android:layout_width="158dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/sfprodisplaylight"
        android:text="@string/reservado_desde"
        android:textAlignment="textStart"
        android:textColor="#81C3F8"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/view" />

    <TextView
        android:id="@+id/locationHotelTextView4"
        style="@style/TextAppearance.AppCompat.Title"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="@font/sfprodisplaysemibold"
        android:text="@string/_22_09_2019_a_24_09_2019"
        android:textAlignment="textStart"
        android:textColor="#FFFFFF"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="@+id/locationHotelTextView3"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/locationHotelTextView3" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="20dp"
        android:layout_height="0dp"
        android:background="@color/defaultBackgroundColor"
        android:scaleType="fitXY"
        android:src="@drawable/editicn"
        app:layout_constraintBottom_toBottomOf="@+id/locationHotelTextView4"
        app:layout_constraintDimensionRatio="h,1:1"
        app:layout_constraintStart_toEndOf="@+id/locationHotelTextView4" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="20dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:background="@color/defaultBackgroundColor"
        android:scaleType="fitXY"
        android:src="@drawable/addressicn"
        app:layout_constraintBottom_toBottomOf="@+id/locationHotelTextView3"
        app:layout_constraintDimensionRatio="h,1:1"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/locationHotelTextView3" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="20dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:background="@color/defaultBackgroundColor"
        android:scaleType="fitXY"
        android:src="@drawable/phoneicn"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton"
        app:layout_constraintDimensionRatio="h,1:1"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/imageButton" />

</androidx.constraintlayout.widget.ConstraintLayout>